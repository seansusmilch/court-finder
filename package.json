{
  "name": "court-finder",
  "version": "0.0.1",
  "author": "Sean Susmilch",
  "packageManager": "bun@1.2.19",
  "private": true,
  "type": "module",
  "scripts": {
    "ruler:apply": "bunx @intellectronica/ruler@latest apply --local-only",
    "dev": "concurrently -n S,C \"bun run dev:backend\" \"bun run dev:frontend\"",
