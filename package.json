{
  "name": "court-finder",
  "version": "0.0.1",
  "author": "Sean Susmilch",
  "packageManager": "bun@1.2.19",
  "private": true,
  "type": "module",
  "scripts": {
    "ruler:apply": "bunx @intellectronica/ruler@latest apply --local-only",
    "dev": "concurrently -n S,C \"bun run dev:backend\" \"bun run dev:frontend\"",
    "dev:backend": "convex dev",
    "dev:backend:setup": "convex dev --configure --until-success",
    "dev:frontend": "vite --port=3001",
    "build": "vite build",
    "serve": "vite preview",
    "start": "vite",
    "check-types": "tsc --noEmit",
    "generate-pwa-assets": "pwa-assets-generator"
  },
  "dependencies": {
    "@auth/core": "0.37.0",
    "@convex-dev/auth": "^0.0.88",
    "@convex-dev/migrations": "^0.2.9",
    "@hookform/resolvers": "^5.2.1",
    "@mapbox/search-js-react": "^1.3.0",
    "@radix-ui/react-progress": "^1.1.7",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-form": "^1.19.2",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-router": "^1.131.27",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.26.1",
    "lucide-react": "^0.473.0",
    "mapbox-gl": "^3.14.0",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-map-gl": "^8.0.4",
    "react-zoom-pan-pinch": "^3.7.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "vite-plugin-pwa": "^1.0.3",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@tanstack/react-router-devtools": "^1.131.27",
    "@tanstack/router-plugin": "^1.131.27",
    "@types/node": "^22.17.2",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@vite-pwa/assets-generator": "^1.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "postcss": "^8.5.6",
    "typescript": "^5.9.2",
    "vite": "^6.3.5"
